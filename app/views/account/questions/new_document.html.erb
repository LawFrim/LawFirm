<div class="col-md-6 col-md-offset-2 well">
    <h4>合同审核</h4>

    <hr>


  <%= simple_form_for  [:account, @question] do |l| %>

  <div class="form-group">
      <%= l.input :attachment ,label: '合同附件' %>
      <%= l.input :content, label: '相关描述',input_html: { class: "form-control"} %>



  </div>
  <%= l.submit "提交", class: "btn  btn-secondary text-uppercase flat animation", data: { disable_with: "提交..." } %>
  <% end %>



</div>
